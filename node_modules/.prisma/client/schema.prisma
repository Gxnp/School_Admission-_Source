datasource db {
  provider = "postgresql" // หรือเปลี่ยนเป็นฐานข้อมูลที่คุณต้องการ
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Application {
  id               String  @id @default(cuid())
  name             String
  email            String
  phone            String
  address          String
  previousSchool   String
  gradeApplyingFor String
  profilePicture   String?
  dateapply        String
}
